{
  "name": "electron-gifs",
  "version": "0.1.0",
  "private": true,
  "devDependencies": {
    "electron": "^6.0.0",
    "electron-packager": "^14.0.4",
    "react": "^16.8.6",
    "react-dom": "^16.8.6",
    "react-scripts": "^3.0.1"
  },
  "dependencies": {
    "cors": "^2.8.5",
    "express": "^4.17.1",
    "fluent-ffmpeg": "^2.1.2",
    "fs-extra": "^8.1.0",
    "gm": "^1.23.1",
    "youtube-dl": "^2.0.0"
  },
  "homepage": "./",
  "main": "src/electron-main.js",
  "scripts": {
    "start": "electron .",
    "dev": "npm run react-start & bash watchman-simple.sh node src/videoAPI.js; npm run react-stop",
    "build": "npm run build-react && npm run build-electron",
    "build-electron": "electron-packager . --overwrite",
    "build-react": "react-scripts build",
    "update-build": "react-scripts build && cp -r build src electron-gifs-darwin-x64/electron-gifs.app/Contents/Resources/app/",
    "test": "react-scripts test --env=jsdom",
    "react-start": "react-scripts start; npm run react-stop",
    "react-stop": "pkill -f 'start.js'"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  }
}
